{
  "name": "termyn/timekeeper",
  "description": "A lightweight library with a time service",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Termyn\\Timekeeper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Termyn\\Timekeeper\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpstan/phpstan": "1.8.*",
    "phpstan/phpstan-phpunit": "1.1.*",
    "phpunit/phpunit": "9.5.*",
    "symplify/easy-coding-standard": "11.1.*"
  }
}
